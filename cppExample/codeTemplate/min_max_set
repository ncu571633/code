#include <set>
#include <iostream>

int main()
{
    // set::set<int> s; // increasing
    // or std::set<int, std::less<int>> s; // increasing
    std::set<int, std::greater<int>> s;
    for (int i=0; i<10; i++)
        s.insert(i);
    
    for(auto& e: s)
    {
        std::cout << e << std::endl;
    }
    
    return 0;
}
